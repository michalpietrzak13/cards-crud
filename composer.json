{
  "name": "example/cards-crud",
  "type": "project",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "laravel/sanctum": "^4.0",
    "laravel/breeze": "^2.0",
    "fruitcake/laravel-cors": "^4.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "phpunit/phpunit": "^11.0"
  },
  "scripts": {
